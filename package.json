{
  "name": "node-crud-api",
  "version": "1.0.0",
  "description": "Simple CRUD API using in-memory database",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "start:dev": "npm run dev",
    "start:prod": "npm run build:prod & node dist/index.js",
    "start:multi": "npm run build:prod & node dist/index.js --multi"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bumble-sakh/node-crud-api.git"
  },
  "keywords": [
    "nodejs",
    "Bumble",
    "crud"
  ],
  "author": "Bumble-sakh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Bumble-sakh/node-crud-api/issues"
  },
  "homepage": "https://github.com/Bumble-sakh/node-crud-api#readme",
  "devDependencies": {
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "nodemon": "^2.0.20",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
